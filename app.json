{
  "expo": {
    "name": "transferx",
    "slug": "transferx",
    "owner": "solstar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/completesize.png",
      "resizeMode": "contain",
      "backgroundColor": "#081412"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#081412"
      },
      "package": "com.transferx.com"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "71fcffe0-78d5-47b0-adfb-c5f90a253ce6"
      },
      "env": {
        "API_URL": "@env API_URL"
      }
    },
    "plugins": ["expo-font"]
  }
}
